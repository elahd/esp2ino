{
    "configurations": [
        {
            "name": "devcontainer",
            "includePath": [
                "${workspaceFolder}/source/**",
                "${env:IDF_PATH}/components/**",
                "${workspaceFolder}/source/main",
                "${workspaceFolder}/source/static",
                "${env:IDF_PATH}/tools/**"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": "",
                "path": [
                    "${workspaceFolder}/../../source/firmware/backend/static",
                    "${workspaceFolder}/source/main",
                    "${workspaceFolder}/source/static"
                ]
            },
            "defines": [
                "__ESP_FILE__"
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "compileCommands": "${workspaceFolder}/source/build/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}